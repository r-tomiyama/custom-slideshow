{
  "main": "index.js",
  "author": "peko",
  "engines": {
    "node": ">= 12",
    "npm": ">= 6.14"
  },
  "scripts": {
    "start": "ts-node index.ts",
    "fix": "run-p fix:*",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint --ext js,ts .",
    "lint:prettier": "prettier -l \"**/*.{json,md,yml}\"",
    "lint:type": "tsc",
    "migrate:create": "node node_modules/db-migrate/bin/db-migrate create --",
    "migrate:up": "node node_modules/db-migrate/bin/db-migrate up",
    "migrate:down": "node node_modules/db-migrate/bin/db-migrate down"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.7.1",
    "@typescript-eslint/eslint-plugin": "4.29.2",
    "@typescript-eslint/parser": "4.29.2",
    "db-migrate": "0.11.12",
    "db-migrate-mysql": "2.1.2",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.24.1",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.3.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "express": "^4.17.1"
  }
}
